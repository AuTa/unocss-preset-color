{
  "name": "unocss-preset-color",
  "version": "0.0.0",
  "type": "module",
  "description": "A preset for Unocss",
  "author": "AuTa <auta520@live.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AuTa/unocss-preset-color.git"
  },
  "bugs": {
    "url": "https://github.com/AuTa/unocss-preset-color/issues"
  },
  "keywords": ["unocss"],
  "files": ["dist", "src"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"

  "peerDependencies": {
    "unocss": ">= 0.56.0 < 1",
    "@unocss/preset-mini": ">= 0.56.0 < 1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@biomejs/biome": "^1.9.4",
    "@rslib/core": "^0.2.2",
    "typescript": "~5.7.2",
    "unocss": "^0.65.3",
    "vitest": "^3.0.0-beta.3"
  }
